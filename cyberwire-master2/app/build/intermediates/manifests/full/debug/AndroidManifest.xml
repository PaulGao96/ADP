<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.opensourcecurrency.hack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/cyberwire"
        android:label="@string/app_name" >
        <activity
            android:name="org.opensourcecurrency.hack.Cyberwire"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.opensourcecurrency.hack.Prefs"
            android:label="@string/settings_label" />
        <activity android:name="org.opensourcecurrency.hack.Send" />
        <activity android:name="org.opensourcecurrency.hack.Transactions" />
        <activity android:name="org.opensourcecurrency.hack.Profile" />
        <activity android:name="org.opensourcecurrency.hack.WebViewActivity" />
        <activity android:name="org.opensourcecurrency.hack.AddProvider" />
    </application>

</manifest>